# Staging Environment Configuration

# Database Configuration
DB_HOST=staging-postgres.yourdomain.com
DB_PORT=5432
DB_USER=libuser_staging
DB_PASSWORD=${STAGING_DB_PASSWORD}
DB_NAME=libmngmt_staging
DB_SSLMODE=require

# Redis Configuration
REDIS_HOST=staging-redis.yourdomain.com
REDIS_PORT=6379
REDIS_PASSWORD=${STAGING_REDIS_PASSWORD}
REDIS_DB=0
REDIS_ENABLED=true

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8080

# Logging
LOG_LEVEL=debug

# Environment
ENV=staging
API_BASE_URL=https://staging-api.yourdomain.com
